<html>
<head>
    <title>$title</title>
    <meta http-equiv="Pragma" content="no-cache"/>
    <meta http-equiv="Expires" content="-1"/>
    <a:If ifTrue="$isDebuggingMode">\
        <meta name="AribaPage" content="true"/>
    </a:If>\
</head>
<body>
    <!--- Put special marker around form so it's parsed out by XMLHTTPUpdateSource -->\
    <a:PrimitiveString value="<!--@&@-->"/>\
    <form method="post" id="formRedirectForm" name="$^formName:formRedirectForm" action="$formActionUrl" enctype="$enctype" target="$target">
        <a:For list="$formValues" index="$currIndex">\
            <a:HiddenFormParameter name="$currentName" value="$currentValue"/>
        </a:For>\
        <a:IncludeContent name="extraFormParams" required="$false"/>
    </form>\
    <a:PrimitiveString value="<!--@&@-->"/>\
    <a:IncludeContent/>
    <a:GenericContainer tagName="script" type="text/javascript">
        if (window.name == "AWRefreshFrame" || window.ariba_IR) {
            <a:CurrWindowDecl/>
            ariba.Refresh.iFrameFormSubmit("<a:String value="$^formName:formRedirectForm"/>");
        }
        else {
            document.<a:String value="$^formName:formRedirectForm"/>.submit();
        }
    </a:GenericContainer>
</body>
</html>
